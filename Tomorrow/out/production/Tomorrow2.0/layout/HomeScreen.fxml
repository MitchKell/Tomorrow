          </Tab>
            <Tab id="medTab" text="Medium Priority">
              <content>
                <AnchorPane id="medTabBack" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab id="lowTab" text="Low Priority">
